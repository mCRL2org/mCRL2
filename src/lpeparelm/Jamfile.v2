project lpeparelm
       : source-location .
       : requirements
          <include>$(BOOST)
       : build-dir $(BUILD_TOP)
       ;

exe lpeparelm
       : lpeparelm.cpp
         /libraries/temporary//print
         /libraries/lpe//lpe
       : <source>/boost/program_options//boost_program_options
         <source>/boost/filesystem//boost_filesystem
         <source>/root//mcrl2_revision
         <squadt_support>enable
         <include>.
       ;

install_with_libraries lpeparelm ;
