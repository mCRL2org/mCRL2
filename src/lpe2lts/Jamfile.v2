project lpe2lts
       : source-location .
       : build-dir $(BUILD_TOP)
       : requirements
          <include>$(BOOST)
       ;

exe lpe2lts
       : lpe2lts.cpp
         /libraries/temporary//dataelm
         /libraries/temporary//nextstate
         /libraries/temporary//print
         /libraries/temporary//trace
         /libraries/lts//lts
         /libraries/sip//sipt
       : <source>/root//mcrl2_revision
       ;

install_with_libraries lpe2lts ;
