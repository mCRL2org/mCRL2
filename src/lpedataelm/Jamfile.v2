project lpedataelm
       : source-location .
       : build-dir $(BUILD_TOP)
       : requirements
          <include>$(BOOST)
       ;

exe lpedataelm
       : lpedataelm.cpp
         /libraries/temporary//dataelm
         /libraries/temporary//mcrl2_basic
       : <source>/boost/program_options//boost_program_options
         <source>/site-config//mcrl2_revision.h
         <include>.
       ;

install_with_libraries install : lpedataelm ;
