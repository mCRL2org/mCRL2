<?xml version="1.0"?>
<!-- A very basic Petri net, without 'special' constructions or deadlocks -->
<pnml>
	<net type="EPNML11" id="example1">
		<place id="pl01">
			<name>
				<text>place01</text>
			</name>
			<type>
				<text>channel</text>
			</type>
		</place>
		<place id="pl02">
			<name>
				<text>place02</text>
			</name>
			<initialMarking>
				<text>1</text>
			</initialMarking>
		</place>
		<transition id="tr01">
			<name>
				<text>trans01</text>
			</name>
			<type>
				<text>AND</text>
			</type>
		</transition>
		<transition id="tr02">
			<name>
				<text>trans02</text>
			</name>
		</transition>
		<arc id="arc01" source="pl01" target="tr01"/>
		<arc id="arc02" target="pl02" source="tr01"/>
		<arc id="arc03" source="pl02" target="tr02"/>
		<arc id="arc04" target="pl01" source="tr02"/>
	</net>
</pnml>
