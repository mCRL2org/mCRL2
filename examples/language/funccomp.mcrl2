%                                   Function composition
%                                   --------------------
%
% This is an mCRL2 specification of the composition f Â° g for function f of
% type B -> C and function g of type A -> B.

sort
  A,B,C;

map
  comp: (B -> C) # (A -> B) -> (A -> C);

var
  f: B -> C;
  g: A -> B;
eqn
  comp(f,g) = lambda x:A.f(g(x));

init
  delta;
