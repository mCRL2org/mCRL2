<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="SimpleNet" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <place id="p1">
      <name>
        <text>Place1</text>
      </name>
      <initialMarking>
        <text>1</text>
      </initialMarking>
    </place>
    <place id="p2">
      <name>
        <text>Place2</text>
      </name>
      <initialMarking>
        <text>0</text>
      </initialMarking>
    </place>
    <transition id="t1">
      <name>
        <text>Transition1</text>
      </name>
    </transition>
    <arc id="a1" source="p1" target="t1">
    </arc>
    <arc id="a2" source="t1" target="p2">
    </arc>
  </net>
