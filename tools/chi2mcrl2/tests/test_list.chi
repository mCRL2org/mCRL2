model tst()=
|[ chan a: bool
:: dummy(a)
]|

proc dummy(chan k!: bool)=
|[ var a: [nat] = [0,2,3],
       b: nat = 0
:: a := []
 ; a := [1,2,3]
 ; b in [1,2] -> skip
 ; b in a -> skip
 ; b in [] -> skip
 ; b := 10
 ; a := a ++ a ++ [b]
 ; ([] = a) -> skip
 ; ([b] = a) -> skip
 ; [1,2] = [1,23] -> skip
 ; b := len([1,2])
 ; b := hd([4,2])
 ; a := tl([4,2])
 ; b := hr([1,2,3])
 ; a := tr([1,2,3])
 ; a := take([1,2,3],2)
 ; a := [1,2,3,4,4] -- [1,2,4]
]|

